      USBAEP                    :: Bit      -- USBAEP
      _                         :: Bits 4   -- (Reserved)
      MPSIZ                     :: Bits 11  -- MPSIZ

    INT 0x368 - device endpoint-1 interrupt register
      _                         :: Bits 25  -- (Reserved)
      B2BSTUP                   :: Bit      -- B2BSTUP
