      ACKM                      :: Bit      -- ACK response received/transmitted interrupt mask
      NAKM                      :: Bit      -- NAK response received interrupt mask
      STALLM                    :: Bit      -- STALL response received interrupt mask
      _                         :: Bit      -- (Reserved)
